    "humps": "^2.0.1",
    "react": "^18.2.0",
    "react-alice-carousel": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.2"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@types/humps": "^2.0.6",
